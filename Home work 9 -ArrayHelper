package shiuor8;

public class ArrayHelper {
	//targil 1
	public static void printArr(int [] arr) {
		for(int i=0;i<arr.length;i++) {
		    
			System.out.println(arr[i]);
		}
	}
	
	//targil2
	public static int[] creatArr(int x) {
		int [] arr = new int[x];
		return arr;
	}
	//targil3
	public static int [] hacpaltatArr(int [] arr) {
		int[] newArr = new int[arr.length];
		for(int i=0;i<arr.length;i++) {
			newArr[i]=arr[i]*arr[i];
			
		}
		printArr(newArr);
		return newArr;
}

}
